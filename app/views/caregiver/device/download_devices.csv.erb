<% headers = ['Device Name', 'Property Name', 'Property Value', 'Property Metric', 'Alerted At', 'Acknowledged At'] %>
<%= CSV.generate_line(headers, col_sep: ",") %>
<% @properties.each do |p| %>
<%= CSV.generate_line([p.device_mapping.device.name, p.property.key, p.value, p.property.metric, UserUtils::get_day_to_show(p.created_at, params[:time_offset]).strftime("%m/%d/%Y %H:%M"), UserUtils::get_day_to_show(p.dismiss_time, params[:time_offset]).strftime("%m/%d/%Y %H:%M")], row_sep: "", col_sep: ",") %>
<% end %>