<input type="hidden" id="<%= prefix_id %>_id" name="<%= prefix_id %>_id" value="<%= targetValue if !targetValue.nil? %>"/>
<input type="text" <%= if defined?(onSelect) then "data-onselect=#{onSelect}" end %> <%= if defined?(required) then "required='#{required}'" end %> placeholder="<%= placeholder if !placeholder.nil? %>" class="form-control" id="<%= prefix_id %>Select" name="<%= prefix_id %>Select" role="autocomplete" data-target="<%= prefix_id %>_id" data-service="<%= service %>" value="<%= targetLabel %>"/>